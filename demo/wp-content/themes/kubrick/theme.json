{
  "$schema": "https://schemas.wp.org/wp/6.3/theme.json",
  "settings": {
    "appearanceTools": false,
    "border": {
      "color": true,
      "radius": true
    },
    "color": {
      "defaultPalette": false,
      "custom": false,
      "palette": [
        {
          "color": "#eeeeee",
          "slug": "grey",
          "name": "Grey"
        },
        {
          "color": "#afafaf",
          "slug": "grey-2",
          "name": "Grey-2"
        },
        {
          "color": "#6ea2dc",
          "slug": "blue",
          "name": "Blue"
        },
        {
          "color": "#f8f8f8",
          "slug": "grey-3",
          "name": "Grey-3"
        },
        {
          "color": "#4774a9",
          "slug": "blue-2",
          "name": "Blue-2"
        },
        {
          "color": "#e0e0e0",
          "slug": "grey-4",
          "name": "Grey-4"
        }
      ],
      "gradients": [
        {
          "gradient": "linear-gradient(180deg, rgba(110, 162, 220, 1) 0%, rgba(71, 116, 169, 1) 100%)",
          "slug": "blue",
          "name": "Blue"
        },
        {
          "gradient": "linear-gradient(180deg, rgba(254, 254, 254, 1) 0%, rgba(227, 227, 227, 1) 100%)",
          "slug": "grey",
          "name": "Grey"
        }
      ],
      "defaultGradients": false
    },
    "layout": {
      "contentSize": "450px",
      "wideSize": "740px"
    },
    "shadow": {
      "defaultPresets": false,
      "presets": []
    },
    "spacing": {
      "customSpacingSize": false,
      "spacingScale": {
        "increment": 1.5,
        "mediumStep": 1.5,
        "operator": "*",
        "steps": 7,
        "unit": "rem"
      },
      "spacingSizes": [],
      "units": ["%", "px", "em", "rem", "vh", "vw"]
    },
    "typography": {
      "customFontSize": false,
      "dropCap": false,
      "fontFamilies": [
        {
          "fontFamily": "'Lucida Grande', Verdana, Arial, Sans-Serif;",
          "name": "Kubrick",
          "slug": "kubrick"
        }
      ],
      "fontSizes": [],
      "fontStyle": false,
      "fontWeight": false,
      "letterSpacing": false,
      "textDecoration": false,
      "textTransform": false
    },
    "useRootPaddingAwareAlignments": false
  },
  "styles": {
    "css": "img {\n   max-width: 100%;\n}"
  },
  "version": 2
}
